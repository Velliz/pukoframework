{
  "name": "puko/framework",
  "description": "MVC framework for quick and fast PHP application development",
  "type": "framework",
  "version": "1.1.1",
  "license": "GPL-3.0+",
  "authors": [
    {
      "name": "velliz",
      "email": "diditvelliz@outlook.com",
      "homepage": "http://velliz.github.io",
      "role": "Lead Developer"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/Velliz/pukoframework"
    }
  ],
  "minimum-stability": "RC",
